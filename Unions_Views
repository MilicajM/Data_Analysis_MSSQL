SELECT *
FROM dbo.October2018_inventory
UNION
SELECT *
FROM dbo.November2018_inventory
UNION
SELECT *
FROM dbo.December2018_inventory
UNION 
-- 2019 --
SELECT *
FROM dbo.January2019_inventory
UNION
SELECT *
FROM dbo.February2019_inventory
UNION
SELECT *
FROM dbo.March2019_inventory
UNION
SELECT *
FROM dbo.April2019_inventory
UNION
SELECT *
FROM dbo.May2019_inventory
UNION
SELECT *
FROM dbo.June2019_inventory
UNION 
SELECT *
FROM dbo.July2019_inventory
UNION
SELECT *
FROM dbo.August2019_inventory
UNION
SELECT * 
FROM dbo.September2019_inventory
UNION
SELECT *
FROM dbo.October2019_inventory
UNION
SELECT *
FROM dbo.November2019_inventory
UNION
SELECT *
FROM dbo.December2019_inventory
UNION 
-- 2020 --
SELECT *
FROM dbo.January2020_inventory
UNION
SELECT *
FROM dbo.February2020_inventory
UNION
SELECT *
FROM dbo.March2020_inventory
UNION
SELECT *
FROM dbo.April2020_inventory
UNION
SELECT *
FROM dbo.May2020_inventory
UNION
SELECT *
FROM dbo.June2020_inventory
UNION 
SELECT *
FROM dbo.July2020_inventory
UNION
SELECT *
FROM dbo.August2020_inventory
UNION
SELECT * 
FROM dbo.September2020_inventory -- this is 2020
UNION
SELECT *
FROM dbo.October2020_inventory
UNION
SELECT *
FROM dbo.November2020_inventory
UNION
SELECT *
FROM dbo.December2020_inventory
UNION 
-- 2021 --
SELECT *
FROM dbo.January2021_inventory
UNION
SELECT *
FROM dbo.February2021_inventory
UNION
SELECT *
FROM dbo.March2021_inventory
