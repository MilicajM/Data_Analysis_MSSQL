-- Data taken from Zillow from the year 2018 to end of 2022 in monthly fashion

-- Create view for inventory using union on many tables


CREATE VIEW inventory AS 
(
-- 2018 --
SELECT *
FROM dbo.March2018_inventory
UNION
SELECT *
FROM dbo.April2018_inventory
UNION
SELECT *
FROM dbo.May2018_inventory
UNION
SELECT *
FROM dbo.June2018_inventory
UNION 
SELECT *
FROM dbo.July2018_inventory
UNION
SELECT *
FROM dbo.August2018_inventory
UNION
SELECT * 
FROM dbo.September2018_inventory
